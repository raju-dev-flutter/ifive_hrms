<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Ifive Hrms</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>ifive_hrms</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>NFCReaderUsageDescription</key>
	<string>Your app requires access to NFC capabilities.</string>
	<key>NSBluetoothPeripheralUsageDescription</key>
	<string>Your app requires access to Bluetooth.</string>
	<key>NSCalendarsUsageDescription</key>
	<string>Your app requires access to the calendar.</string>
	<key>NSCameraUsageDescription</key>
	<string>Your app requires access to the camera.</string>
	<key>NSContactsUsageDescription</key>
	<string>Your app requires access to contacts.</string>
	<key>NSFaceIDUsageDescription</key>
	<string>Your app requires access to Face ID.</string>
	<key>NSHealthShareUsageDescription</key>
	<string>Your app requires access to health data.</string>
	<key>NSHealthUpdateUsageDescription</key>
	<string>Your app requires write access to health data.</string> 
	<key>NSLocationTemporaryUsageDescriptionDictionary</key>
	<dict>
		<key>NSLocationTemporaryUsageDescriptionTypeBackground</key>
		<string>Your app requires access to location services for background use.</string>
		<key>NSLocationTemporaryUsageDescriptionTypeForeground</key>
		<string>Your app requires access to location services when in use.</string>
	</dict>
    <!-- Location Permissions -->
    <key>NSLocationWhenInUseUsageDescription</key>
    <string>We need your location to show your position on the map.</string>
    <key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
    <string>We need your location to track your position even when the app is in the background.</string>
    <key>NSLocationAlwaysUsageDescription</key>
    <string>We need your location to track your position even when the app is in the background.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Your app requires access to the microphone.</string>
	<key>NSMotionUsageDescription</key>
	<string>Your app requires access to motion data.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>Your app requires write access to the photo library.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Your app requires read/write access to the photo library.</string>
	<key>NSRemindersUsageDescription</key>
	<string>Your app requires access to reminders.</string>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UIStatusBarStyle</key>
	<string></string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
	</array>
	<!-- Notifications Permissions -->
    <key>UNNotificationAlertSound</key>
    <true/>
    <key>UNNotificationAlertBadge</key>
    <true/>
    <key>UNNotificationAlertAllowsAlerts</key>
    <true/> 
    <!-- Background Modes -->
    <key>UIBackgroundModes</key>
    <array>
    	<string>remote-notification</string>
    </array> 
    <!-- Firebase Configuration -->
    <key>FirebaseAppDelegateProxyEnabled</key>
    <false/>

</dict>
</plist>
